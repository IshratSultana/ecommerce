<style>
    .wrapper {
        width: 100%;
        min-height: 1vh;
        display: flex;
        align-items: center;
    }

    .loader {
        display: inline-block;
        position: relative;
        width: 50px;
        height: 50px;
        margin: auto;
    }

    .loader:after {
        content: " ";
        display: block;
        border-radius: 50%;
        width: 0;
        height: 0;
        margin: 6px;
        box-sizing: border-box;
        border: 30px solid #FBC91A;
        border-color: #FBC91A transparent #FBC91A transparent;
        animation: loader 1.2s infinite;
    }

    @keyframes loader {
        0% {
            transform: rotate(0);
            animation-timing-function: cubic-bezier(0.55, 0.055, 0.675, 0.19);
        }
        50% {
            transform: rotate(900deg);
            animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
        }
        100% {
            transform: rotate(1800deg);
        }
    }
</style>
