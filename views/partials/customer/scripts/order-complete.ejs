<script>
    window.dataLayer = window.dataLayer || [];

    function gtag() {
        dataLayer.push(arguments);
    }

    gtag('js', new Date());

    gtag('config', 'UA-23581568-13');

    $(document).ready(function () {
        var referrer = document.referrer;
        // console.log(referrer)

        const cart = JSON.parse(localStorage.getItem('cart'))
        console.log(referrer.indexOf('checkout'))
        if (referrer.indexOf('checkout') === -1) {
            if (Object.keys(cart.products).length === 0 && !cart.checkout) {
                window.location.replace("/cart");
            } else if (Object.keys(cart.products).length > 0) {
                window.location.replace("/checkout");
            } else {
                // console.log("Haw maw kaw")
                window.location.replace("/cart");
            }
        } else {
            // console.log("Permission has")
        }
    });
</script>
